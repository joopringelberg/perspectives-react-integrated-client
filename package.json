    "build": "./build.sh",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "./release.sh"
  },
  "author": "Joop Ringelberg <joop.ringelberg@gmail.com>",
  "repository": {
    "type": "git",
