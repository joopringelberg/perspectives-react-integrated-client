{
  "name": "InPlace",
  "version": "0.3.11",
  "description": "A client with the core built-in.",
  "private": true,
  "main": "public/app-starter.js",
  "homepage": "https://inplace.one",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --watch",
    "electron": "WEBPACK_DEVSERVER_PORT=5678 electron . -d=true",
